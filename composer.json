{
    "name": "mwillbanks/mw-common",
    "description": "A module contains various components for ZF2",
    "type": "library",
    "keywords": [
        "zf2", "MwCommon"
    ],
    "homepage": "https://github.com/mwillbanks/MwCommon",
    "authors": [
        {
            "name": "Mike Willbanks",
            "email": "mike@digitalstruct.com",
            "homepage": "http://blog.digitalstruct.com/"
        }
    ],
    "autoload": {
        "psr-0": {
            "MwCommon\\": "src/"
        },
        "classmap": [
            "./"
        ]
    },
    "repositories": [
        {
            "type": "composer",
            "url": "http://packages.zendframework.com/"
        }
    ],
    "require": {
        "php": ">=5.3.3",
        "zendframework/zend-loader": "2.*",
        "zendframework/zend-filter": "2.*",
        "zendframework/zend-validator": "2.*",
        "zendframework/zend-i18n": "2.*",
        "zendframework/zend-view": "2.*"
    },
    "require-dev": {
        "zendframework/zendframework": "2.*"
    }
}
